,\\n       \"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(\"redirectUris\"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(\": [ \"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(\"http://localhost:3000/callback\"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(\" ],\\n       \"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(\"scopes\"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(\": [ \"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(\"apps:beta\"),a(\"span\",{pre:!0,attrs:{class:\"token entity\",title:'\\\\\"'}},[t._v('\\\\\"')]),t._v(' ]\\n       }\"')]),t._v(\" \"),a(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\"\\\\\")]),t._v(\"\\n     \"),a(\"span\",{pre:!0,attrs:{class:\"token string\"}},[t._v(\"'https://api.snyk.io/v3/orgs/<ORG_ID>/apps?version='\")]),t._v(\"\\n\")])])]),a(\"p\",[t._v(\"The response from Snyk contains two important values necessary to complete our Snyk App's integration: \"),a(\"code\",[t._v(\"clientId\")]),t._v(\" and \"),a(\"code\",[t._v(\"clientSecret\")]),t._v(\". Store these values somewhere safe. This is the only time you will ever see your \"),a(\"code\",[t._v(\"clientSecret\")]),t._v(\" from Snyk. As a warning, \"),a(\"strong\",[t._v(\"never share your \"),a(\"code\",[t._v(\"clientSecret\")]),t._v(\" publicly\")]),t._v(\". This is used to authenticate your App with Snyk.\")]),t._v(\" \"),a(\"p\",[t._v(\"Now that we've registered as a Snyk App, we can start adjusting our TypeScript project to allow users to authorize it.\")]),t._v(\" \"),a(\"h2\",{attrs:{id:\"user-authorization-with-a-snyk-app\"}},[a(\"a\",{staticClass:\"header-anchor\",attrs:{href:\"#user-authorization-with-a-snyk-app\"}},[t._v(\"#\")]),t._v(\" User Aut